    "data_evento_str = input(\"Digite a data do evento: \")\n",
    "\n",
    "data_evento = datetime.strptime(data_evento_str, \"%d/%m/%Y\")\n",
    "\n",
    "data_agora = datetime.now()\n",
    "\n",
    "dias_faltando = (data_evento - data_agora).days\n",
    "\n",
    "print(dias_faltando)"
